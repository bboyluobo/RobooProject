workspace 'RobooProject.xcworkspace'
project 'RobooProject/RobooProject.xcodeproj'


target 'RobooProject' do
inhibit_all_warnings!
#platform :ios,'8.0'
platform :ios, '10.3'
project 'RobooProject/RobooProject.xcodeproj'
# 网络请求
pod 'AFNetworking'
pod 'ASIHTTPRequest'

# 网络监控
pod 'Reachability'
# RAC
pod 'ReactiveObjC', '~> 3.0.0'
#字典转模型
pod 'MJExtension'
# 下拉刷新
pod 'MJRefresh'
# 图片加载
pod 'SDWebImage'
pod 'SDWebImage/GIF'

#用于适配
pod 'Masonry'

# 表情键盘
pod 'HMEmoticon'
# 正则匹配点击
pod 'KILabel'

# YYKit
#pod 'YYKit'
pod 'YYImage'
pod 'YYText'
pod 'YYAsyncLayer'
pod 'YYCategories'

# 通用链接跳转
#pod 'MagicWindowSDK'

# 数据库
pod 'FMDB'

# toast
pod 'Toast'

# 蒙版
pod 'MBProgressHUD'

# 无限轮播
pod 'SDCycleScrollView'

# 键盘管理
pod 'IQKeyboardManager'

# 唯一设备标识
pod 'FCUUID'

# 全局滑动
pod 'FDFullscreenPopGesture'

# 列表视频
pod 'ZFPlayer', '~> 2.0.0'

pod 'iflyMSC'

end

target 'RobooSwiftProject' do
inhibit_all_warnings!
project 'RobooSwiftProject/RobooSwiftProject.xcodeproj'
use_frameworks!
pod 'RxSwift'
pod 'RxCocoa'
pod 'RxDataSources'

end
